{
    "version": "1.5.0-alpha",
    "compileOnSave": false,
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "noEmitOnError": true,
        "removeComments": true,
        "noLib": false,
        "outDir": "./built/",
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "./**/*.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "./typings/angularjs/angular.d.ts",
        "./typings/bootstrap/bootstrap.d.ts",
        "./typings/jquery/jquery.d.ts",
        "./typings/tsd.d.ts"
    ]
}